	<!-- swiper js -->
	<script>
		$(function(){
			$('.page2 > .bxslider').bxSlider({
				auto: true,
				autoControls: false,
				stopAutoOnClick: false,
				pager: false,
				mode:'fade',
				//slideWidth: 1300
			});

			$('.page5 > .bxslider').bxSlider({
				auto: true,
				autoControls: false,
				stopAutoOnClick: false,
				pager: false,
				mode:'fade'
				//slideWidth: 1214
			});

			// 메인이미지
			var _mainImg = $('.imgBox > .imgcnt').children('img'),
				n = _mainImg.length,
				i = 1,
				t;
			var _sec = 4;

			(function loop() {
				t = setTimeout(function(){
					var _order = i++%n;
					_mainImg.not(':eq(' + _order + ')').fadeOut(1500);
					_mainImg.eq(_order).fadeIn(1500);
					loop();
				}, _sec * 1000);
			}());
		});

		function popupWindows(url, w, h, popupName, resizable)
		{
			var sx = (screen.width - w) / 2;
			var sy = (screen.height - h) / 2;
			mywin = window.open(url, popupName, 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable='+resizable+',width='+w+',height='+h+',top='+sy+',left='+sx);
			if (!mywin)
				alert("차단된 팝업창을 허용해 주세요.");
			else
				mywin.focus();
		}

		function popupWebMessinger()
		{
			var isLogin = '';

			if(isLogin != '1')
			{
				if (confirm("로그인이 필요한 서비스입니다. 로그인 하시겠습니까?"))
				{
					redirectURL = document.location.href;
					document.location.href = "/login/loginfull.php?rurl="+encodeURIComponent(redirectURL);
				}
				else
				{
					//pass....
				}
			}
			else
			{
				javascript:popupWindows('https://main.wim.nate.com/popup2/index.html', 820 , 670, 'Nateon','yes');
			}
		}
	</script>
